#=====================
# urxvt settings
#=====================

# Load plugins
URxvt.perl-ext: clipboard-osc,selection-to-clipboard,default,matcher

# Overall settings
URxvt.scrollBar: false

URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

# Font Settings
urxvt.font: xft:DejaVu Sans Mono:pixelsize=11

# Color Settings
urxvt.background: #002b36
urxvt.foreground: #ffffff

!! black dark/light
*.color0: black
*.color1: red3
*.color2: green3
*.color3: yellow3
*.color4: blue2
*.color5: magenta3
*.color6: cyan3
*.color7: grey90
*.color8: grey50
*.color9: red
*.color10: green
*.color11: yellow
*.color12: #5560C8
*.color13: magenta
*.color14: cyan
*.color15: white
#=====================
# /urxvt settings
#=====================
